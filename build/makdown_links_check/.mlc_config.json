{
  "ignorePatterns": [
    {
      "pattern": "^/src/api/sdks"
    },
    {
      "pattern":"^http://localhost"
    },
    {
      "pattern":"^https://localhost"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/src/"
    },
    {
      "pattern": "^file:\/\/.*\/img\/",
      "replacement": "{{BASEURL}}/src/.vuepress/public/img/"
    },
    {
      "pattern": "^file:\/\/.*\/media\/",
      "replacement": "{{BASEURL}}/src/.vuepress/public/media/"
    }
  ]
}